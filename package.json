{
  "name": "github-italics-theme",
  "displayName": "GitHub Theme with Italics",
  "description": "This VSCode theme merges GitHub color scheme with Tomorrow Operator Mono's text decoration scheme",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.45.0"
  },
  "publisher": "gitKendra",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#ffffff",
    "theme": "light"
  },
  "homepage": "https://github.com/gitKendra/github-italics-vscode-theme/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/gitKendra/github-italics-vscode-theme.git"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "github",
    "light",
    "dark",
    "italic"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Github Light (Italics)",
        "uiTheme": "vs",
        "path": "./themes/github-light-with-italics-color-theme.json"
      },
      {
        "label": "Github Dark (Italics)",
        "uiTheme": "vs",
        "path": "./themes/github-dark-with-italics-color-theme.json"
      }
    ]
  }
}
